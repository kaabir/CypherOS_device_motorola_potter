<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds.  Do not translate.
     NOTE: The naming convention is "config_camelCaseValue". Some legacy
     entries do not follow the convention, but all new entries should. -->

<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    
    <!-- Determines whether the device supports fingerprint navigation -->
    <bool name="config_supportsFPNavigation">true</bool>

    <!-- Key codes of fingerprint gestures. If the gesture is not supported set it to 0. -->
    <integer name="config_fpSingleTapKeyCode">616</integer>
    <integer name="config_fpDoubleTapKeyCode">626</integer>
    <integer name="config_fpLongpressKeyCode">617</integer>
    <integer name="config_fpSwipeUpKeyCode">0</integer>
    <integer name="config_fpSwipeDownKeyCode">0</integer>
    <integer name="config_fpSwipeLeftKeyCode">621</integer>
    <integer name="config_fpSwipeRightKeyCode">620</integer>

    <!-- Whether the device supports fingerprint gestures -->
    <bool name="config_supportsFPGestures">true</bool>

    <!-- Whether to to intercept inputs on screen off. -->
    <bool name="config_enableKeyHandler">true</bool>

</resources>
